{
  "components": [
    {
      "components": [
        {
          "class": "justify-content-start align-items-center",
          "components": [
            {
              "class": "col-md-6 d-flex align-items-center",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "code": "sidebar0.Toggle();",
                        "type": "execute"
                      },
                      {
                        "code": "body0.Toggle();",
                        "type": "execute"
                      }
                    ]
                  },
                  "name": "sidebar-toggle0",
                  "type": "sidebar-toggle"
                },
                {
                  "name": "label0",
                  "text": "RadzenCRM",
                  "type": "label"
                }
              ],
              "name": "leftCol",
              "type": "col"
            },
            {
              "class": "col-md-6 text-lg-right",
              "components": [
                {
                  "name": "profilemenu0",
                  "template": [
                    {
                      "email": "${Security.User?.Name}",
                      "type": "gravatar"
                    }
                  ],
                  "type": "profilemenu",
                  "visible": "${Security.IsAuthenticated()}"
                }
              ],
              "name": "rightCol",
              "type": "col"
            }
          ],
          "name": "headerRow",
          "type": "row"
        }
      ],
      "name": "header0",
      "type": "header"
    },
    {
      "components": [
        {
          "name": "main",
          "type": "content-container"
        }
      ],
      "name": "body0",
      "type": "body"
    },
    {
      "components": [
        {
          "name": "panelmenu0",
          "style": {
            "height": "100%"
          },
          "type": "panelmenu"
        }
      ],
      "name": "sidebar0",
      "type": "sidebar"
    },
    {
      "components": [
        {
          "name": "footerText",
          "text": "RadzenCRM, Copyright &copy; 2019",
          "type": "label"
        }
      ],
      "name": "footer0",
      "type": "footer"
    }
  ],
  "events": {
    "load": [
      {
        "name": "Culture",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      },
      {
        "code": "Culture = await JSRuntime.InvokeAsync<string>(\"Radzen.getCulture\");",
        "type": "execute"
      }
    ]
  },
  "name": "Main"
}