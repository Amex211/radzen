{
  "access": [],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Orders",
          "type": "heading"
        },
        {
          "addText": "Add",
          "allowFiltering": true,
          "allowPaging": true,
          "allowSorting": true,
          "columns": [
            {
              "filterValue": "${defaultOrderIDFilterValue}",
              "property": "OrderID",
              "title": "Order ID",
              "type": "integer"
            },
            {
              "filterProperty": "Customer.CompanyName",
              "property": "CustomerID",
              "sortProperty": "Customer.CompanyName",
              "template": "${data.Customer?.CompanyName}",
              "title": "Customer",
              "type": "string"
            },
            {
              "filterProperty": "Employee.LastName",
              "property": "EmployeeID",
              "sortProperty": "Employee.LastName",
              "template": "${data.Employee?.LastName}",
              "title": "Employee",
              "type": "string"
            },
            {
              "format": "date-time",
              "property": "OrderDate",
              "template": "${data.OrderDate | utcDate}",
              "title": "Order Date",
              "type": "string"
            },
            {
              "format": "date-time",
              "property": "RequiredDate",
              "template": "${data.RequiredDate | utcDate}",
              "title": "Required Date",
              "type": "string"
            },
            {
              "format": "date-time",
              "property": "ShippedDate",
              "template": "${data.ShippedDate | utcDate}",
              "title": "Shipped Date",
              "type": "string"
            },
            {
              "filterProperty": "Shipper.CompanyName",
              "property": "ShipVia",
              "sortProperty": "Shipper.CompanyName",
              "template": "${data.Shipper?.CompanyName}",
              "title": "Shipper",
              "type": "string"
            },
            {
              "format": "decimal",
              "property": "Freight",
              "title": "Freight",
              "type": "number"
            },
            {
              "property": "ShipName",
              "title": "Ship Name",
              "type": "string"
            },
            {
              "property": "ShipAddress",
              "title": "Ship Address",
              "type": "string"
            },
            {
              "property": "ShipCity",
              "title": "Ship City",
              "type": "string"
            },
            {
              "property": "ShipRegion",
              "title": "Ship Region",
              "type": "string"
            },
            {
              "property": "ShipPostalCode",
              "title": "Ship Postal Code",
              "type": "string"
            },
            {
              "property": "ShipCountry",
              "title": "Ship Country",
              "type": "string"
            }
          ],
          "count": "${getOrdersCount}",
          "data": "${getOrdersResult}",
          "emptyText": "No records to display.",
          "events": {
            "loadData": [
              {
                "name": "Northwind.getOrders",
                "parameters": [
                  {
                    "name": "$top",
                    "value": "event.top"
                  },
                  {
                    "name": "$skip",
                    "value": "event.skip"
                  },
                  {
                    "name": "$expand",
                    "value": "Customer,Employee,Shipper"
                  },
                  {
                    "name": "$count",
                    "value": "event.top != null && event.skip != null"
                  },
                  {
                    "name": "$filter",
                    "value": "${event.filter}"
                  },
                  {
                    "name": "$orderby",
                    "value": "${event.orderby}"
                  },
                  {
                    "name": "$expand",
                    "value": "Customer,Employee,Shipper"
                  }
                ],
                "then": [
                  {
                    "name": "getOrdersResult",
                    "type": "set",
                    "value": "${result.value}"
                  },
                  {
                    "name": "getOrdersCount",
                    "type": "set",
                    "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                  }
                ],
                "type": "invoke"
              }
            ]
          },
          "name": "grid0",
          "pageSize": 10,
          "type": "grid"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "condition": "",
        "name": "defaultOrderIDFilterValue",
        "type": "set",
        "value": "10250"
      },
      {
        "name": "Northwind.getOrders",
        "parameters": [
          {
            "name": "$top",
            "value": "this.grid0.allowPaging ? this.grid0.pageSize : null"
          },
          {
            "name": "$skip",
            "value": "this.grid0.allowPaging ? 0 : null"
          },
          {
            "name": "$count",
            "value": "this.grid0.allowPaging"
          },
          {
            "name": "$expand",
            "value": "Customer,Employee,Shipper"
          },
          {
            "name": "$filter",
            "value": "OrderID eq ${defaultOrderIDFilterValue}"
          }
        ],
        "then": [
          {
            "name": "getOrdersResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getOrdersCount",
            "type": "set",
            "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Orders"
}