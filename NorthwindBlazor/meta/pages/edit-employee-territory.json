{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "closeLabel",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Item no longer available.",
                  "type": "label",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "type": "close-popup"
                      }
                    ]
                  },
                  "icon": "",
                  "name": "closeButton",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Close",
                  "type": "button",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Employee",
                              "component": "EmployeeID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getEmployeesResult}",
                              "textProperty": "LastName",
                              "valueProperty": "EmployeeID",
                              "placeholder": "Choose Employee",
                              "type": "dropdown",
                              "name": "EmployeeID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              }
                            },
                            {
                              "type": "required-validator",
                              "name": "EmployeeIDRequiredValidator",
                              "component": "EmployeeID",
                              "text": "EmployeeID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Territory",
                              "component": "TerritoryID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTerritoriesResult}",
                              "textProperty": "TerritoryDescription",
                              "valueProperty": "TerritoryID",
                              "placeholder": "Choose Territory",
                              "type": "dropdown",
                              "name": "TerritoryID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              }
                            },
                            {
                              "type": "required-validator",
                              "name": "TerritoryIDRequiredValidator",
                              "component": "TerritoryID",
                              "text": "TerritoryID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "name": "canEdit",
                            "type": "set",
                            "value": "!(result.status == 400)",
                            "valueType": "bool"
                          },
                          {
                            "detail": "Unable to update EmployeeTerritory",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.updateEmployeeTerritory",
                        "parameters": [
                          {
                            "name": "EmployeeID",
                            "value": "${parameters.EmployeeID}"
                          },
                          {
                            "name": "TerritoryID",
                            "value": "${parameters.TerritoryID}"
                          },
                          {
                            "name": "EmployeeTerritory",
                            "value": "${employeeterritory}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${employeeterritory}"
                          }
                        ],
                        "type": "invoke",
                        "argType": "EmployeeTerritory"
                      }
                    ]
                  },
                  "data": "${employeeterritory}",
                  "visible": "${employeeterritory != null && canEdit}",
                  "itemType": "EmployeeTerritory"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "Northwind.getEmployeeTerritoryByEmployeeIDAndTerritoryID",
        "parameters": [
          {
            "name": "EmployeeID",
            "value": "${parameters.EmployeeID}"
          },
          {
            "name": "TerritoryID",
            "value": "${parameters.TerritoryID}"
          }
        ],
        "then": [
          {
            "name": "employeeterritory",
            "type": "set",
            "value": "${result}",
            "valueType": "EmployeeTerritory"
          }
        ],
        "error": [
          {
            "name": "canEdit",
            "type": "set",
            "value": "!(result.status == 400)",
            "valueType": "bool"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getEmployees",
        "parameters": [],
        "then": [
          {
            "name": "getEmployeesResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<Employee>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getTerritories",
        "parameters": [],
        "then": [
          {
            "name": "getTerritoriesResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<Territory>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Employee Territory",
  "layout": "Main"
}