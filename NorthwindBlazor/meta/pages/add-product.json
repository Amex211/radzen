{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Product Name",
                              "component": "ProductName",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 40,
                              "type": "textbox",
                              "name": "ProductName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              }
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductNameRequiredValidator",
                              "component": "ProductName",
                              "text": "ProductName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Supplier",
                              "component": "SupplierID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getSuppliersResult}",
                              "textProperty": "CompanyName",
                              "valueProperty": "SupplierID",
                              "placeholder": "Choose Supplier",
                              "type": "dropdown",
                              "name": "SupplierID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Category",
                              "component": "CategoryID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getCategoriesResult}",
                              "textProperty": "CategoryName",
                              "valueProperty": "CategoryID",
                              "placeholder": "Choose Category",
                              "type": "dropdown",
                              "name": "CategoryID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Quantity Per Unit",
                              "component": "QuantityPerUnit",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 20,
                              "type": "textbox",
                              "name": "QuantityPerUnit",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Unit Price",
                              "component": "UnitPrice",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "number",
                              "valueFormat": "decimal",
                              "type": "numeric",
                              "name": "UnitPrice",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Units In Stock",
                              "component": "UnitsInStock",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "integer",
                              "valueFormat": "int16",
                              "type": "numeric",
                              "name": "UnitsInStock",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Units On Order",
                              "component": "UnitsOnOrder",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "integer",
                              "valueFormat": "int16",
                              "type": "numeric",
                              "name": "UnitsOnOrder",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Reorder Level",
                              "component": "ReorderLevel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "integer",
                              "valueFormat": "int16",
                              "type": "numeric",
                              "name": "ReorderLevel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Discontinued",
                              "component": "Discontinued",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "boolean",
                              "type": "checkbox",
                              "name": "Discontinued",
                              "triState": true
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Product!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.createProduct",
                        "parameters": [
                          {
                            "name": "Product",
                            "value": "${product}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${product}"
                          }
                        ],
                        "type": "invoke",
                        "argType": "Product"
                      }
                    ]
                  },
                  "data": "${product}",
                  "visible": "${product != null}",
                  "itemType": "Product"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "Northwind.getSuppliers",
        "parameters": [],
        "then": [
          {
            "name": "getSuppliersResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<Supplier>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getCategories",
        "parameters": [],
        "then": [
          {
            "name": "getCategoriesResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<Category>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "product",
        "type": "set",
        "value": "new Product()",
        "valueType": "Product"
      }
    ]
  },
  "name": "Add Product",
  "layout": "Main"
}