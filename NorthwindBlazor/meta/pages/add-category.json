{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Category Name",
                              "component": "CategoryName",
                              "name": "CategoryNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 15,
                              "type": "textbox",
                              "name": "CategoryName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${category.CategoryName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CategoryNameRequiredValidator",
                              "component": "CategoryName",
                              "text": "CategoryName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${category.Description}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Picture",
                              "component": "Picture",
                              "name": "PictureLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "Byte[]",
                              "type": "file-input",
                              "name": "Picture",
                              "value": "${category.Picture}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Category!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.createCategory",
                        "parameters": [
                          {
                            "name": "Category",
                            "value": "${category}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${category}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${category}",
                  "visible": "${category != null}",
                  "itemType": "NorthwindBlazor.Models.Northwind.Category"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "category",
        "type": "set",
        "value": "new NorthwindBlazor.Models.Northwind.Category()",
        "valueType": "NorthwindBlazor.Models.Northwind.Category"
      }
    ]
  },
  "name": "Add Category",
  "layout": "Main"
}