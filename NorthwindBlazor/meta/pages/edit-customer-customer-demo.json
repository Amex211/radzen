{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Edit Customer Customer Demo"
        },
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "closeLabel",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Item no longer available.",
                  "type": "label",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "path": "customer-customer-demos",
                        "type": "navigate"
                      }
                    ]
                  },
                  "icon": "",
                  "name": "closeButton",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Close",
                  "type": "button",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Customer",
                              "component": "CustomerID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getCustomersResult}",
                              "textProperty": "CompanyName",
                              "valueProperty": "CustomerID",
                              "placeholder": "Choose Customer",
                              "type": "dropdown",
                              "name": "CustomerID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              }
                            },
                            {
                              "type": "required-validator",
                              "name": "CustomerIDRequiredValidator",
                              "component": "CustomerID",
                              "text": "CustomerID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Customer Demographic",
                              "component": "CustomerTypeID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getCustomerDemographicsResult}",
                              "textProperty": "CustomerDesc",
                              "valueProperty": "CustomerTypeID",
                              "placeholder": "Choose CustomerDemographic",
                              "type": "dropdown",
                              "name": "CustomerTypeID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              }
                            },
                            {
                              "type": "required-validator",
                              "name": "CustomerTypeIDRequiredValidator",
                              "component": "CustomerTypeID",
                              "text": "CustomerTypeID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "path": "customer-customer-demos",
                                    "type": "navigate"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "name": "canEdit",
                            "type": "set",
                            "value": "!(result.status == 400)",
                            "valueType": "bool"
                          },
                          {
                            "detail": "Unable to update CustomerCustomerDemo",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.updateCustomerCustomerDemo",
                        "parameters": [
                          {
                            "name": "CustomerID",
                            "value": "${parameters.CustomerID}"
                          },
                          {
                            "name": "CustomerTypeID",
                            "value": "${parameters.CustomerTypeID}"
                          },
                          {
                            "name": "CustomerCustomerDemo",
                            "value": "${customercustomerdemo}"
                          }
                        ],
                        "then": [
                          {
                            "path": "customer-customer-demos",
                            "type": "navigate"
                          }
                        ],
                        "type": "invoke",
                        "argType": "CustomerCustomerDemo"
                      }
                    ]
                  },
                  "data": "${customercustomerdemo}",
                  "visible": "${customercustomerdemo != null && canEdit}",
                  "itemType": "CustomerCustomerDemo"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "Northwind.getCustomerCustomerDemoByCustomerIDAndCustomerTypeID",
        "parameters": [
          {
            "name": "CustomerID",
            "value": "${parameters.CustomerID}"
          },
          {
            "name": "CustomerTypeID",
            "value": "${parameters.CustomerTypeID}"
          }
        ],
        "then": [
          {
            "name": "customercustomerdemo",
            "type": "set",
            "value": "${result}",
            "valueType": "CustomerCustomerDemo"
          }
        ],
        "error": [
          {
            "name": "canEdit",
            "type": "set",
            "value": "!(result.status == 400)",
            "valueType": "bool"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getCustomers",
        "parameters": [],
        "then": [
          {
            "name": "getCustomersResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<Customer>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getCustomerDemographics",
        "parameters": [],
        "then": [
          {
            "name": "getCustomerDemographicsResult",
            "type": "set",
            "value": "${result.value}",
            "valueType": "IEnumerable<CustomerDemographic>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Customer Customer Demo",
  "layout": "Main"
}