                      {
                        "error": [
                          {
                            "detail": "Unable to update Supplier",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.updateSupplier",
                        "parameters": [
                          {
                            "name": "SupplierID",
                            "value": "${parameters.SupplierID}"
                          },
                          {
                            "name": "Supplier",
                            "value": "${supplier}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${supplier}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "NorthwindBlazor.Models.Northwind.Supplier",
                  "data": "${supplier}",
                  "visible": "${supplier != null}"
                }
              ]
