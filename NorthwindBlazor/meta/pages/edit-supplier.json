  ],
  "events": {
    "load": [
      {
        "name": "Northwind.getSupplierBySupplierID",
        "parameters": [
          {
            "name": "SupplierID",
            "value": "${parameters.SupplierID}"
          }
        ],
        "then": [
          {
            "name": "supplier",
            "type": "set",
            "value": "${result}",
            "valueType": "NorthwindBlazor.Models.Northwind.Supplier"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
